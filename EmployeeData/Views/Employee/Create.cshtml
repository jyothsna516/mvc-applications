@model EmployeeData.Models.Employee
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Create</h1>
@using (Html.BeginForm())
{
    <table class="table">
        <tr>
            <td>EmployeeId</td>
            <td>@Html.TextBoxFor(m => m.EmployeeId)</td>
        </tr>
        <tr>
            <td>Name</td>
            <td>@Html.TextBoxFor(m => m.Name)</td>
        </tr>
        <tr>
            <td>Desigination</td>
            <td>@Html.DropDownListFor(m => m.Desigination, ViewBag.Desigination as SelectList)</td>

        </tr>
        <tr>
            <td>ProjectName</td>
            <td>@Html.DropDownListFor(m => m.ProjectName, ViewBag.ProjectName as SelectList)</td>
        </tr>

        <tr>
            <td>Password</td>
            <td>@Html.Password("Pwd")</td>
        </tr>
        <tr>
            <td colspan="2">
                <input type="submit" value="Register" />
            </td>
        </tr>
        
        
    </table>
}

